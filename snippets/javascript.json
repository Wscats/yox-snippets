{
    "import": {
        "prefix": "import",
        "body": [
            "import $1 from '$2'"
        ],
        "description": "import ... from ..."
    },
    "yox": {
        "prefix": "newYox",
        "body": [
            "new Yox({",
            "\t$1",
            "})"
        ],
        "description": "new Yox()"
    },
    "addSpecialEvent": {
        "prefix": "Yox.dom.addSpecialEvent",
        "body": [
            "Yox.dom.addSpecialEvent(\"${1:definition}\", {",
            "\t${2:definition}: function (element, listener) { ${3:handle} }",
            "})"
        ],
        "description": "Yox.dom.addSpecialEvent"
    },
    "yoxDomOn": {
        "prefix": "Yox.dom.on",
        "body": [
            "Yox.dom.on(${1:element}, '${2:event}', ${3:listener})"
        ],
        "description": "Yox.dom.on"
    },
    "yoxDomOff": {
        "prefix": "Yox.dom.on",
        "body": [
            "Yox.dom.off(${1:element}, '${2:event}', ${3:listener})"
        ],
        "description": "Yox.dom.off"
    },
    "yoxDirective": {
        "prefix": "Yox.directive",
        "body": [
            "Yox.directive(${1:element}, {",
            "\tbind: function (node, directive, vnode) { ${2:handle} },",
            "\tunbind: function (node, directive, vnode) { ${2:handle} }",
            "})"
        ],
        "description": "Yox.directive"
    },
    "yoxComponent": {
        "prefix": "Yox.component",
        "body": [
            "Yox.component(${1:element}, {})"
        ],
        "description": "Yox.component"
    },
    "yoxTransition": {
        "prefix": "Yox.transition",
        "body": [
            "Yox.transition(${1:name}, {",
            "\tenter: function (node) { ${2:handle} },",
            "\tleave: function (node, done) { ${3:handle} }",
            "})"
        ],
        "description": "Yox.transition"
    },
    "template": {
        "prefix": "template",
        "body": [
            "template: \"$1\""
        ],
        "description": "template"
    },
    "name": {
        "prefix": "name",
