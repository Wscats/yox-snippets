{
    "import": {
        "prefix": "import",
        "body": [
            "import $1 from '$2'"
        ],
        "description": "import ... from ..."
    },
    "yox": {
        "prefix": "newYox",
        "body": [
            "new Yox({",
            "\t$1",
            "})"
        ],
        "description": "new Yox()"
    },
    "addSpecialEvent": {
        "prefix": "Yox.dom.addSpecialEvent",
        "body": [
            "Yox.dom.addSpecialEvent(\"${1:definition}\", {",
            "\t${2:definition}: function (element, listener) { ${3:handle} }",
            "})"
        ],
        "description": "Yox.dom.addSpecialEvent"
    },
    "yoxDomOn": {
        "prefix": "Yox.dom.on",
        "body": [
            "Yox.dom.on(${1:element}, '${2:event}', ${3:listener})"
        ],
        "description": "Yox.dom.on"
    },
    "yoxDomOff": {
        "prefix": "Yox.dom.on",
        "body": [
            "Yox.dom.off(${1:element}, '${2:event}', ${3:listener})"
        ],
        "description": "Yox.dom.off"
    },
    "template": {
        "prefix": "template",
        "body": [
            "template: \"$1\""
        ],
        "description": "template"
    },
    "data": {
        "prefix": "data",
        "body": [
            "data: {",
            "\t${1:key}: ${2:value}",
            "}"
        ],
        "description": "data"
    },
    "filters": {
        "prefix": "filters",
        "body": [
            "filters: {",
            "\t${1:definition}: function (value) { ${2:handle} }",
            "}"
        ],
        "description": "filters"
    },
    "computed": {
        "prefix": "computed",
        "body": [
            "computed: {",
            "\t${1:definition}: function (value) { ${2:handle} }",
            "}"
        ],
        "description": "computed"
    },
    "methods": {
        "prefix": "methods",
        "body": [
            "methods: {",
            "\t${1:definition}: function (value) { ${2:handle} }",
            "}"
        ],
        "description": "methods"
    },
    "events": {
        "prefix": "events",
        "body": [
            "events: {",
            "\t${1:definition}: function (value) { ${2:handle} }",
            "}"
        ],
        "description": "events"
    },
    "partials": {
        "prefix": "partials",
        "body": [
            "partials: {",
            "\t${1:definition}: {${2:key}: ${3:value}}",
            "}"
        ],
        "description": "partials"
    }
}