{
    "template": {
        "prefix": "template",
        "body": [
            "<template>",
            "\t<${1:div}$2>",
            "\t\t$0",
            "\t</${1:div}>",
            "</template>"
        ],
        "description": "template element"
    },
    "mustache": {
        "prefix": "{{",
        "body": [
            "{{$1}}"
        ],
        "description": "template element"
    },
    "event": {
        "prefix": [
            "on-event",
            "#event"
        ],
        "body": [
            "on-${1:event}=\"${2:handle}\""
        ],
        "description": "Expects: Function | Inline Statement"
    },
    "if": {
        "prefix": "#if",
        "body": [
            "{{#if ${1:condition}}}",
            "\tA",
            "{{else if ${2:condition}}}",
            "\tB",
            "{{else}}",
            "\tC",
            "{{/if}}"
        ],
        "description": "Expects: boolean"
    },
    "each": {
        "prefix": "#each",
        "body": [
            "{{#each ${1:items}:${2:item}}}",
            "\t${3:item}",
            "{{/each}}"
        ],
        "description": "Expects: Array | Object | number | string"
    },
    "partial": {
        "prefix": "#partial",
        "body": [
            "{{#partial $1}}",
            "\t$2",
            "{{/partial}}"
        ],
        "description": "Expects: Array | Object | number | string"
    },
    "annotation": {
        "prefix": "<!-",
        "body": [
            "<!-- ${1:annotation} -->"
        ],
        "description": "Expects: String"
    }
}